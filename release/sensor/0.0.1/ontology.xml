  </rdf:Description>
  <rdf:Description rdf:about="http://linked.earth/ontology#InorganicSensor">
    <rdfs:comment xml:lang="en">An inorganic sensor. Examples of inorganic sensors include: karst, snow, watershed, ice sheet, etc.</rdfs:comment>
    <owl:disjointWith rdf:resource="http://linked.earth/ontology#OrganicSensor"/>
    <rdfs:subClassOf rdf:resource="http://linked.earth/ontology#ProxySensor"/>
    <rdfs:label xml:lang="en">Inorganic sensor</rdfs:label>

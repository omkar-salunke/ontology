  <rdf:Description rdf:about="http://linked.earth/ontology#LakeSediment">
    <rdfs:label xml:lang="en">Lake sediment</rdfs:label>
    <rdfs:comment xml:lang="en">&lt;p&gt;&lt;a rel="nofollow" class="external text" href="https://en.wikipedia.org/wiki/Lake"&gt;Lake&lt;/a&gt; &lt;a rel="nofollow" class="external text" href="https://en.wikipedia.org/wiki/Sediment"&gt;sediment&lt;/a&gt; proxies provide widespread, continuous records of terrestrial environment variability. A variety of sensors are used to indicate past water temperature, physical properties, biology, and chemistry within the &lt;a rel="nofollow" class="external text" href="https://en.wikipedia.org/wiki/Lake"&gt;lake&lt;/a&gt; environment as well as changes in vegetation and precipitation in the &lt;a rel="nofollow" class="external text" href="https://en.wikipedia.org/wiki/Drainage_basin"&gt;catchment&lt;/a&gt; area.
&lt;/p&gt;</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://linked.earth/ontology#ProxyArchive"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>

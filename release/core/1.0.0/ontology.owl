<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY dcterms "http://purl.org/dc/terms/" >
    <!ENTITY vann "http://purl.org/vocab/vann/" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>


<rdf:RDF xmlns="http://linked.earth/ontology#"
     xml:base="http://linked.earth/ontology"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:dcterms="http://purl.org/dc/terms/">
    <owl:Ontology rdf:about="http://linked.earth/ontology#">
        <dcterms:license>http://creativecommons.org/licenses/by-nc-sa/2.0/</dcterms:license>
        <rdfs:comment xml:lang="en">The Linked Earth Ontology aims to provide a common vocabulary for annotating paleoclimatology data</rdfs:comment>
        <vann:preferredNamespacePrefix>le</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri>http://linked.earth/ontology#</vann:preferredNamespaceUri>
        <owl:versionInfo xml:lang="en">1.0.0</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2000/01/rdf-schema#Literal -->

    <rdf:Description rdf:about="&rdfs;Literal">
        <owl:equivalentClass rdf:resource="&xsd;string"/>
    </rdf:Description>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://linked.earth/ontology#archivedIn -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#archivedIn">
        <rdfs:label xml:lang="en">archivedIn</rdfs:label>
        <rdfs:comment xml:lang="en">How was the climate or chronological information encoded into the Archive?</rdfs:comment>
        <rdfs:range rdf:resource="http://linked.earth/ontology#Archive"/>
        <rdfs:domain rdf:resource="http://linked.earth/ontology#Proxy"/>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#author -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#author">
        <rdfs:label xml:lang="en">author</rdfs:label>
        <rdfs:comment xml:lang="en">Author of the resource</rdfs:comment>
        <rdfs:range rdf:resource="http://linked.earth/ontology#Person"/>
        <rdfs:subPropertyOf rdf:resource="&dcterms;creator"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://linked.earth/ontology#Dataset"/>
                    <rdf:Description rdf:about="http://linked.earth/ontology#Publication"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#basedOn -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#basedOn">
        <rdfs:label xml:lang="en">basedOn</rdfs:label>
        <rdfs:comment xml:lang="en">How are the data obtained?</rdfs:comment>
        <rdfs:domain rdf:resource="http://linked.earth/ontology#Data"/>
        <rdfs:range rdf:resource="http://linked.earth/ontology#Proxy"/>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#calibratedFrom -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#calibratedFrom">
        <rdfs:label xml:lang="en">calibratedFrom</rdfs:label>
        <rdfs:comment xml:lang="en">Property that indicates how the measured variable was calibrated</rdfs:comment>
        <rdfs:range rdf:resource="http://linked.earth/ontology#CalibrationModel"/>
        <rdfs:domain rdf:resource="http://linked.earth/ontology#MeasuredVariable"/>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#calibratedWith -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#calibratedWith">
        <rdfs:label xml:lang="en">calibratedWith</rdfs:label>
        <rdfs:comment xml:lang="en">How was the variable calibrated to represent climate or chronology?</rdfs:comment>
        <rdfs:range rdf:resource="http://linked.earth/ontology#CalibrationModel"/>
        <rdfs:domain rdf:resource="http://linked.earth/ontology#InferredVariable"/>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#calibrationReferences -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#calibrationReferences">
        <rdfs:label xml:lang="en">calibrationReferences</rdfs:label>
        <rdfs:comment xml:lang="en">The publication that describes the calibration. The string should follow the model set in the publication class</rdfs:comment>
        <rdfs:domain rdf:resource="http://linked.earth/ontology#CalibrationModel"/>
        <rdfs:range rdf:resource="http://linked.earth/ontology#Publication"/>
        <rdfs:subPropertyOf rdf:resource="&dcterms;references"/>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#chronModeledBy -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#chronModeledBy">
        <rdfs:label xml:lang="en">chronModeledBy</rdfs:label>
        <rdfs:comment xml:lang="en">How was the chron data modeled?</rdfs:comment>
        <rdfs:range rdf:resource="http://linked.earth/ontology#ChronModel"/>
        <rdfs:subPropertyOf rdf:resource="http://linked.earth/ontology#modeledBy"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://linked.earth/ontology#ChronData"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#collectedFrom -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#collectedFrom">
        <rdfs:label xml:lang="en">collectedFrom</rdfs:label>
        <rdfs:comment xml:lang="en">Where was the data in the dataset collected from? Generally, this is the site where the archive was collected. </rdfs:comment>
        <rdfs:domain rdf:resource="http://linked.earth/ontology#Dataset"/>
        <rdfs:range rdf:resource="http://linked.earth/ontology#Location"/>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#contributor -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#contributor">
        <rdfs:label xml:lang="en">contributor</rdfs:label>
        <rdfs:comment xml:lang="en">Contributor of the resource (dataset, publication, etc.)</rdfs:comment>
        <rdfs:domain rdf:resource="http://linked.earth/ontology#Dataset"/>
        <rdfs:range rdf:resource="http://linked.earth/ontology#Person"/>
        <rdfs:subPropertyOf rdf:resource="&dcterms;contributor"/>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#foundInDistributionTable -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#foundInDistributionTable">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">foundInDistributionTable</rdfs:label>
        <rdfs:comment xml:lang="en">Property that indicates that a variable or data is found in a distribution table</rdfs:comment>
        <rdfs:range rdf:resource="http://linked.earth/ontology#DistributionTable"/>
        <rdfs:subPropertyOf rdf:resource="http://linked.earth/ontology#foundInTable"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://linked.earth/ontology#ChronModel"/>
                    <rdf:Description rdf:about="http://linked.earth/ontology#PaleoModel"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#foundInEnsembleTable -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#foundInEnsembleTable">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">foundInEnsembleTable</rdfs:label>
        <rdfs:comment xml:lang="en">Property that indicates that the data or variable described was found in an ensemble table</rdfs:comment>
        <rdfs:range rdf:resource="http://linked.earth/ontology#EnsembleTable"/>
        <rdfs:subPropertyOf rdf:resource="http://linked.earth/ontology#foundInTable"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://linked.earth/ontology#ChronModel"/>
                    <rdf:Description rdf:about="http://linked.earth/ontology#PaleoModel"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#foundInMeasurementTable -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#foundInMeasurementTable">
        <rdfs:label xml:lang="en">foundInMeasurementTable</rdfs:label>
        <rdfs:comment xml:lang="en">Property that indicates that a variable was found in a measurement table</rdfs:comment>
        <rdfs:domain rdf:resource="http://linked.earth/ontology#Data"/>
        <rdfs:range rdf:resource="http://linked.earth/ontology#MeasurementTable"/>
        <rdfs:subPropertyOf rdf:resource="http://linked.earth/ontology#foundInTable"/>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#foundInSummaryTable -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#foundInSummaryTable">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">foundInSummaryTable</rdfs:label>
        <rdfs:comment xml:lang="en">Property that indicates that the describer data or variable was found in a summary table</rdfs:comment>
        <rdfs:range rdf:resource="http://linked.earth/ontology#SummaryTable"/>
        <rdfs:subPropertyOf rdf:resource="http://linked.earth/ontology#foundInTable"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://linked.earth/ontology#ChronModel"/>
                    <rdf:Description rdf:about="http://linked.earth/ontology#PaleoModel"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#foundInTable -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#foundInTable">
        <rdfs:label xml:lang="en">foundInTable</rdfs:label>
        <rdfs:comment xml:lang="en">Property that indicated the table where the described data can be found</rdfs:comment>
        <rdfs:range rdf:resource="http://linked.earth/ontology#DataTable"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://linked.earth/ontology#Data"/>
                    <rdf:Description rdf:about="http://linked.earth/ontology#Model"/>
                    <rdf:Description rdf:about="http://linked.earth/ontology#Variable"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#fundedBy -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#fundedBy">
        <rdfs:label xml:lang="en">fundedBy</rdfs:label>
        <rdfs:comment xml:lang="en">Property that indicated the funding which made the dataset possible</rdfs:comment>
        <rdfs:domain rdf:resource="http://linked.earth/ontology#Dataset"/>
        <rdfs:range rdf:resource="http://linked.earth/ontology#Funding"/>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#hasCode -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#hasCode">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">hasCode</rdfs:label>
        <rdfs:comment xml:lang="en">What is the mathematical formulation that describes the Model?</rdfs:comment>
        <rdfs:domain rdf:resource="http://linked.earth/ontology#Model"/>
        <rdfs:range rdf:resource="http://linked.earth/ontology#SoftwareCode"/>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#hasPart -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#hasPart">
        <rdfs:label xml:lang="en">hasPart</rdfs:label>
        <rdfs:comment>What are the components of the ProxySystemModel?</rdfs:comment>
        <rdfs:domain rdf:resource="http://linked.earth/ontology#ProxySystemModel"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://linked.earth/ontology#ArchiveModel"/>
                    <rdf:Description rdf:about="http://linked.earth/ontology#ObservationModel"/>
                    <rdf:Description rdf:about="http://linked.earth/ontology#SensorModel"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#hasResolution -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#hasResolution">
        <rdfs:label xml:lang="en">hasResolution</rdfs:label>
        <rdfs:comment>What is the Resolution of the Archive?</rdfs:comment>
        <rdfs:domain rdf:resource="http://linked.earth/ontology#Archive"/>
        <rdfs:range rdf:resource="http://linked.earth/ontology#Resolution"/>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#hasUncertainty -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#hasUncertainty">
        <rdfs:label xml:lang="en">hasUncertainty</rdfs:label>
        <rdfs:comment xml:lang="en">What is the Uncertainty associated with the Observations, Model, and Calibration?</rdfs:comment>
        <rdfs:range rdf:resource="http://linked.earth/ontology#Uncertainty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://linked.earth/ontology#CalibrationModel"/>
                    <rdf:Description rdf:about="http://linked.earth/ontology#Instrument"/>
                    <rdf:Description rdf:about="http://linked.earth/ontology#Model"/>
                    <rdf:Description rdf:about="http://linked.earth/ontology#Variable"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#includesChronData -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#includesChronData">
        <rdfs:label xml:lang="en">includesChronData</rdfs:label>
        <rdfs:comment xml:lang="en">Pointer to the chron data included in the dataset</rdfs:comment>
        <rdfs:range rdf:resource="http://linked.earth/ontology#ChronData"/>
        <rdfs:domain rdf:resource="http://linked.earth/ontology#Dataset"/>
        <rdfs:subPropertyOf rdf:resource="http://linked.earth/ontology#includesData"/>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#includesData -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#includesData">
        <rdfs:label xml:lang="en">includesData</rdfs:label>
        <rdfs:comment xml:lang="en">What are the paleoenvironmental and chronological Data included in the Dataset?</rdfs:comment>
        <rdfs:range rdf:resource="http://linked.earth/ontology#Data"/>
        <rdfs:domain rdf:resource="http://linked.earth/ontology#Dataset"/>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#includesPaleoData -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#includesPaleoData">
        <rdfs:label xml:lang="en">includesPaleoData</rdfs:label>
        <rdfs:comment xml:lang="en">The paleo data included as part of a dataset</rdfs:comment>
        <rdfs:domain rdf:resource="http://linked.earth/ontology#Dataset"/>
        <rdfs:range rdf:resource="http://linked.earth/ontology#PaleoData"/>
        <rdfs:subPropertyOf rdf:resource="http://linked.earth/ontology#includesData"/>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#includesVariable -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#includesVariable">
        <rdfs:label xml:lang="en">includesVariable</rdfs:label>
        <rdfs:comment xml:lang="en">What are the Variables included in the DataTable?</rdfs:comment>
        <rdfs:domain rdf:resource="http://linked.earth/ontology#DataTable"/>
        <rdfs:range rdf:resource="http://linked.earth/ontology#Variable"/>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#inferredFrom -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#inferredFrom">
        <rdfs:label xml:lang="en">inferredFrom</rdfs:label>
        <rdfs:comment xml:lang="en">From which measured variable is the current inferred variable obtained?</rdfs:comment>
        <rdfs:domain rdf:resource="http://linked.earth/ontology#InferredVariable"/>
        <rdfs:range rdf:resource="http://linked.earth/ontology#MeasuredVariable"/>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#interpretationReferences -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#interpretationReferences">
        <rdfs:label xml:lang="en">interpretationReferences</rdfs:label>
        <rdfs:comment xml:lang="en">Describes the basis by which the proxy variable is interpreted in terms of climate. </rdfs:comment>
        <rdfs:domain rdf:resource="http://linked.earth/ontology#Interpretation"/>
        <rdfs:range rdf:resource="http://linked.earth/ontology#Publication"/>
        <rdfs:subPropertyOf rdf:resource="&dcterms;references"/>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#interpretedAs -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#interpretedAs">
        <rdfs:label xml:lang="en">interpretedAs</rdfs:label>
        <rdfs:comment xml:lang="en">how is the variable interpreted?</rdfs:comment>
        <rdfs:range rdf:resource="http://linked.earth/ontology#Interpretation"/>
        <rdfs:domain rdf:resource="http://linked.earth/ontology#Variable"/>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#measuredBy -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#measuredBy">
        <rdfs:label xml:lang="en">measuredBy</rdfs:label>
        <rdfs:comment xml:lang="en">What Instrument was used to obtain the MeasuredVariables and Observations?</rdfs:comment>
        <rdfs:range rdf:resource="http://linked.earth/ontology#Instrument"/>
        <rdfs:domain rdf:resource="http://linked.earth/ontology#MeasuredVariable"/>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#measuredOn -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#measuredOn">
        <rdfs:label xml:lang="en">measuredOn</rdfs:label>
        <rdfs:comment xml:lang="en">On what Archive were the Observations measured on?</rdfs:comment>
        <rdfs:range rdf:resource="http://linked.earth/ontology#Archive"/>
        <rdfs:domain rdf:resource="http://linked.earth/ontology#MeasuredVariable"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#modelReferences -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#modelReferences">
        <rdfs:label xml:lang="en">modelReferences</rdfs:label>
        <rdfs:comment xml:lang="en">The publication that describes the model. </rdfs:comment>
        <rdfs:domain rdf:resource="http://linked.earth/ontology#Model"/>
        <rdfs:range rdf:resource="http://linked.earth/ontology#Publication"/>
        <rdfs:subPropertyOf rdf:resource="&dcterms;references"/>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#modeledBy -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#modeledBy">
        <rdfs:label xml:lang="en">modeledBy</rdfs:label>
        <rdfs:comment xml:lang="en">How was the process modeled?</rdfs:comment>
        <rdfs:range rdf:resource="http://linked.earth/ontology#Model"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://linked.earth/ontology#Data"/>
                    <rdf:Description rdf:about="http://linked.earth/ontology#Proxy"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#observedAs -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#observedAs">
        <rdfs:label xml:lang="en">observedAs</rdfs:label>
        <rdfs:comment xml:lang="en">What Observations were made?</rdfs:comment>
        <rdfs:range rdf:resource="http://linked.earth/ontology#Observation"/>
        <rdfs:domain rdf:resource="http://linked.earth/ontology#Proxy"/>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#paleoModeledBy -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#paleoModeledBy">
        <rdfs:label xml:lang="en">paleoModeledBy</rdfs:label>
        <rdfs:comment xml:lang="en">How was the paleoData modeled?</rdfs:comment>
        <rdfs:range rdf:resource="http://linked.earth/ontology#PaleoModel"/>
        <rdfs:subPropertyOf rdf:resource="http://linked.earth/ontology#modeledBy"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://linked.earth/ontology#PaleoData"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#principalInvestigator -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#principalInvestigator">
        <rdfs:label xml:lang="en">principalInvestigator</rdfs:label>
        <rdfs:comment xml:lang="en">Who is the PrincipalInvestigator of the Funding grant?</rdfs:comment>
        <rdfs:domain rdf:resource="http://linked.earth/ontology#Funding"/>
        <rdfs:range rdf:resource="http://linked.earth/ontology#Person"/>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#publishedIn -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#publishedIn">
        <rdfs:label xml:lang="en">publishedIn</rdfs:label>
        <rdfs:comment xml:lang="en">Where was the Dataset published? Property that relates a dataset to the publication where it was published.</rdfs:comment>
        <rdfs:domain rdf:resource="http://linked.earth/ontology#Dataset"/>
        <rdfs:range rdf:resource="http://linked.earth/ontology#Publication"/>
    </owl:ObjectProperty>
    


    <!-- http://linked.earth/ontology#sensingWith -->

    <owl:ObjectProperty rdf:about="http://linked.earth/ontology#sensingWith">
        <rdfs:label xml:lang="en">sensingWith</rdfs:label>
        <rdfs:comment xml:lang="en">How does the proxy system senses the environment?</rdfs:comment>
        <rdfs:domain rdf:resource="http://linked.earth/ontology#Proxy"/>
        <rdfs:range rdf:resource="http://linked.earth/ontology#Sensor"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/terms/contributor -->

    <owl:ObjectProperty rdf:about="&dcterms;contributor">
        <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/terms/"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/terms/creator -->

    <owl:ObjectProperty rdf:about="&dcterms;creator">
        <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/terms/"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/terms/references -->

    <owl:ObjectProperty rdf:about="&dcterms;references">
        <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/terms/"/>
    </owl:ObjectProperty>
    


    <!-- http://www.opengis.net/ont/geosparql#hasGeometry -->

    <owl:ObjectProperty rdf:about="http://www.opengis.net/ont/geosparql#hasGeometry">
        <rdfs:domain rdf:resource="http://www.opengis.net/ont/geosparql#Feature"/>
        <rdfs:range rdf:resource="http://www.opengis.net/ont/geosparql#Geometry"/>
        <rdfs:isDefinedBy rdf:resource="http://www.opengis.net/spec/geosparql/1.0"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://linked.earth/ontology#book -->

    <owl:DatatypeProperty rdf:about="http://linked.earth/ontology#book">
        <rdfs:label xml:lang="en">book</rdfs:label>
        <rdfs:comment xml:lang="en">Book in which the publication was published</rdfs:comment>
        <rdfs:domain rdf:resource="http://linked.earth/ontology#Publication"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    



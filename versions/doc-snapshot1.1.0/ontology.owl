    <!-- http://linked.earth/ontology#ProxyArchive -->

    <owl:Class rdf:about="http://linked.earth/ontology#ProxyArchive">
        <rdfs:label xml:lang="en">Proxy archive</rdfs:label>
        <rdfs:comment xml:lang="en">The medium in which the response of a sensor to environmental forcing is recorded. Sensu Evans et al. (2013).

Examples of archives include: marine sediments, corals, wood, lake sediments, speleothems, glacier ice, etc.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.sciencedirect.com/science/article/pii/S0277379113002011"/>
    </owl:Class>
    



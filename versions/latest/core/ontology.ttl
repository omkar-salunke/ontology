              [ a       owl:Class ;
                owl:unionOf (:Dataset :Publication :SoftwareCode)
              ] ;
      rdfs:label "has DOI"@en ;
      rdfs:range xsd:string ;
      rdfs:subPropertyOf dcterms:identifier .


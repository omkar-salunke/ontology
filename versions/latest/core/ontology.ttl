      rdfs:label "Interpretation"@en .

:Location
      a       owl:Class ;
      rdfs:comment "The geographic data and metadata for the dataset"@en ;
      rdfs:label "Location"@en ;
      rdfs:subClassOf <http://www.opengis.net/ont/geosparql#Feature> , <http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing> .

:contributor
